# Gitconfig
# by Dan Menjivar

[user]
	name = Dan Menjivar
	email = 41385232+danmenjivar@users.noreply.github.com
[color]
  branch = auto
  diff = auto
  status = auto
  showbranch = auto
	ui = true
[init]
	defaultBranch = main
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --all --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  type = cat-file -t
  dump = cat-file -p
